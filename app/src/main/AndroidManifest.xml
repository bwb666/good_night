    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver android:name=".CallAlarm" android:process=".remote"></receiver>
        <activity android:name=".Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UsersInfo_activity"/>
        <activity android:name=".ForgotInfo_activity"/>
        <activity android:name=".Register_Activity"/>
        <activity android:name=".after_login"/>
        <activity android:name=".TabActivity1"/>
        <activity android:name=".TabActivity2"/>
        <activity android:name=".Music"/>
        <activity android:name=".Gedan"/>
        <activity android:name=".MyClock"/>
        <activity android:name=".AlarmAlert" />
        <activity android:name=".Mystati" />
    </application>

</manifest>